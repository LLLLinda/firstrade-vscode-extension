{
	"name": "firstrade-wallet",
	"displayName": "Firstrade Wallet",
	"publisher": "linda-cheung",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"configuration": [
			{
				"title": "Firstrade",
				"properties": {
					"firstrade.scrollSpeed": {
						"type": "integer",
						"description": "Pick scroll speed in seconds",
						"default": 5,
						"enum": [
							5,
							10,
							15,
							30,
							60
						]
					}
				}
			}
		],
		"commands": [
			{
				"command": "firstrade-wallet.login",
				"title": "Firstrade: Login"
			},
			{
				"command": "firstrade-wallet.logout",
				"title": "Firstrade: Logout"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.54.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"firstrade-cli": "^0.6.8"
	}
}